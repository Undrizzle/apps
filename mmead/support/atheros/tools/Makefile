#!/bin/bash -f

# ====================================================================
#
# --------------------------------------------------------------------

TARGETS= *.c
VPATH=
TRASH= *.o *.so *~ *.[0-9][0-9][0-9] t.c *.out *.a
CFLAGS+=
LFLAGS=
CC= arm-linux-gcc

# ====================================================================
# 
# --------------------------------------------------------------------

.PHONY: compile install clean

compile:
	$(CC) $(CFLAGS) -c $(TARGETS)

install: 

clean:
	rm -f ${TRASH} 
